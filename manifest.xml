<package>
  <description brief="Plugin for Quadcopter control">

     rqt_quadcoptergui

  </description>
  <author>Gowtham Garimella</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rqt_quadcoptergui</url>
  <depend package="roscpp"/>
  <depend package="rqt_gui"/>
  <depend package="rqt_gui_cpp"/>
	<depend package="std_msgs"/>
	<depend package="tf"/>
	<depend package="controllers"/>
	<depend package="sensor_msgs"/>
	<depend package="visualization_msgs"/>
	<!--depend package="rqt_quadcopter_parsers"/-->
	<depend package="dynamic_reconfigure"/>

  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
    <cpp lflags="-L${prefix}/lib " cflags="-I${prefix}/include"/>
  </export>

</package>


